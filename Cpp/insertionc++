#include <bits/stdc++.h>
using namespace std;
class node{
    public:
    int data;
    node* next:
};
void list(node*n)
{
    while(n!= NULL){
        cout<<n->data <<" "; n  =n->next;
    }
}
int main(){

    node*head;
    node*one = NULL;
    node*two = NULL;
    node*three = NULL;
    one = new node();
    two = new node();
    three = new node();
    one->data = 20;
    two->data = 30;
    three->data = 40;
    one->next= two;
    two->next=three;
    three->next=NULL;
    head = one;
    list(head);
    return 0;
    





}
